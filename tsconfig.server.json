{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": false,
    "noImplicitAny": false,
    "outDir": "./dist/server",
    "rootDir": "./server",
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": ["server/**/*"]
}

